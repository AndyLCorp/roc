{
  "name": "roc-config",
  "version": "0.0.1-alpha1",
  "description": "Configuration manager for the Roc ecosystem",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "test": "npm run lint && mocha --compilers js:babel-core/register",
    "check": "ncu",
    "docs": "esdoc -c esdoc.json",
    "prepublish": "npm test && npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vgno/roc-config.git"
  },
  "keywords": [
    "roc"
  ],
  "author": "VG",
  "license": "MIT",
  "devDependencies": {
    "babel": "~5.8.21",
    "babel-eslint": "~4.1.5",
    "chai": "~3.4.1",
    "esdoc": "~0.4.3",
    "esdoc-importpath-plugin": "0.0.1",
    "eslint": "~1.9.0",
    "eslint-config-vgno": "~5.0.0",
    "mocha": "~2.3.4",
    "npm-check-updates": "~2.3.0",
    "rimraf": "~2.4.3"
  },
  "dependencies": {
    "colors": "~1.1.2",
    "deep-extend": "~0.4.0",
    "source-map-support": "~0.3.3"
  }
}
