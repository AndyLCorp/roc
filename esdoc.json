{
    "source": "./src",
    "destination": "./esdocs",
    "plugins": [{
            "name": "esdoc-es7-plugin"
        }, {
            "name": "esdoc-importpath-plugin",
            "option": {
                "replaces": [
                    {"from": "^src/", "to": "lib/"}
                ]
            }
        }
    ]
}
